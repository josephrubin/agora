{
  "private": true,
  "name": "agora",
  "description": "NFTs for communities.",
  "license": "Prop (c) Agora",
  "scripts": {
    "build": "remix build",
    "start": "remix-serve build",
    "dev": "./agora.sh && concurrently -n \"remix,gqgen\" -c \"green,magenta\" --kill-others \"remix dev\" \"npm:wcodegen\"",
    "deploy": "cdk deploy",
    "synth": "cdk synth",
    "postinstall": "remix setup node",
    "lint": "eslint .",
    "test": "jest",
    "test-u": "jest -u",
    "codegen": "graphql-codegen --config codegen.yaml",
    "wcodegen": "graphql-codegen --config codegen.yaml --watch",
    "prebuild": "npm run codegen"
  },
